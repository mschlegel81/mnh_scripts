A005114->{begin
  local touched:=[1].toSet;
  local scanned:=3;
  local prev:=1;
save;
  prev+=1;
  while(true,begin
    while(scanned<prev^2,touched|=sumOfProperDividers(scanned+=1));
    prev in touched
    ? prev+=1
    : return prev;
  end);
end}.toIteratableExpression;

