A181391->{begin
  local seq:=[];
save;
  local ocurredAt:=indexOf(seq.trailing=seq).leading;
  local nextTerm:=ocurredAt==[]
                  ? 0
                  : (seq.size-1-max(ocurredAt));
  seq|=nextTerm;
  nextTerm;
end}.toIteratableExpression;

