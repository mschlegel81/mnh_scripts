WordsByChar:=
  [0..2].each(k,ThreeLetterWords.toList.group(k));

findForFirstLine(w:StringList(3))->begin
  print(w);

  local c0_candidates:=WordsByChar[0,w[0]] orElse []; c0_candidates.size=0 ? return void : void;
  local c1_candidates:=WordsByChar[0,w[1]] orElse []; c1_candidates.size=0 ? return void : void;
  local c2_candidates:=WordsByChar[0,w[2]] orElse []; c2_candidates.size=0 ? return void : void;

  cross(c0_candidates,
        c1_candidates,
        c2_candidates).pMap({begin
    local W:=$x.transpose;
    (W[1] in ThreeLetterWords) AND
    (W[2] in ThreeLetterWords) ? W : void;
  end}).unique;

  //.map({print@($x.map(::join).upper&"\n")}) ;
end;
